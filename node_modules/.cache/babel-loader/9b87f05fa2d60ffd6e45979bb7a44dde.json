) of recipes\" :key=\"r.id\">\n    <h1>{{ r.name }}</h1>\n    <h2>ingredients</h2>\n    <div class=\"content\">{{ r.ingredients }}</div>\n    <h2>steps</h2>\n    <div class=\"content\">{{ r.steps }}</div>\n    <button type=\"button\" @click=\"deleteRecipe(index)\">delete</button>\n  </div>\n</template>\n\n<script>\nimport { v4 as uuidv4 } from \"uuid\";\nexport default {\n  name: \"App\",\n  data() {\n    return {\n      recipe: {\n        name: \"\",\n        ingredients: \"\",\n        steps: \"\",\n      },\n      recipes: [],\n    };\n  },\n  computed: {\n    formValid() {\n      const { name, ingredients, steps } = this.recipe;\n      return name && ingredients && steps;\n    },\n  },\n  methods: {\n    addRecipe() {\n      if (!this.formValid) {\n        return;\n      }\n      this.recipes.push({\n        id: uuidv4(),\n        ...this.recipe,\n      });\n    },\n    deleteRecipe(index) {\n      this.recipes.splice(index, 1);\n    },\n  },\n};\n</script>\n\n<style scoped>\n.content {\n  white-space: pre-wrap;\n}\n</style>"],"sourceRoot":""}]}